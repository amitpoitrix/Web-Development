<%- include("partials/header") -%>

<h1>Compose</h1>
<form class="" action="/compose" method="post">

  <div class="mb-3">
    <label for="title1" class="form-label">Title</label>
    <input type="text" class="form-control" id="title1" name="postTitle">
  </div>
  <div class="mb-3">
    <label for="post1" class="form-label">Post</label>
    <textarea name="postBody" class="form-control" id="post1" rows="8" cols="80"></textarea>
  </div>
  <br>
  <button type="submit" class="btn btn-primary" name="button">Publish</button>

</form>

<%- include("partials/footer") -%>
